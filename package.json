{
  "type": "module",
  "name": "flowbite-react-admin-dashboard",
  "version": "1.0.0",
  "descrption": "flowbite-react admin dashboard",
  "homepage": "https://github.com/themesberg/flowbite-react-admin-dashboard",
  "bugs": "https://github.com/themesberg/flowbite-react-admin-dashboard/issues",
  "license": "UNLICENSED",
  "author": "Conner Davis <cdavis@scu.edu>",
  "repository": {
    "type": "git",
    "url": "https://github.com/themesberg/flowbite-react-admin-dashboard.git"
  },
  "scripts": {
    "build": "yarn typecheck && vite build",
    "dev": "cross-env-shell NODE_ENV=development vite --host",
    "start": "cross-env-shell NODE_ENV=development vite --host",
    "format": "prettier --check --ignore-path .gitignore --write .",
    "lint": "eslint --ignore-path .gitignore .",
    "preview": "vite preview --host",
    "typecheck": "tsc",
    "postinstall": "node -r dotenv/config --es-module-specifier-resolution=node --no-warnings _config.gen.cjs && husky install"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 8",
    "yarn": ">= 1"
  },
  "dependencies": {
    "@fancyapps/ui": "^5.0.33",
    "apexcharts": "^3.45.2",
    "axios": "^1.6.7",
    "axios-cache-interceptor": "^1.5.1",
    "classnames": "^2.5.1",
    "flowbite": "^2.2.1",
    "flowbite-react": "^0.7.2",
    "jquery-form-saver": "https://github.com/dimaslanjaka/jquery-form-saver#master",
    "react-apexcharts": "^1.4.1",
    "react-icons": "^5.0.1",
    "react-simple-keyboard": "^3.7.78",
    "simple-keyboard": "^3.7.53",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@types/google.accounts": "^0.0.14",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "@types/sortablejs": "^1.15.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "binary-collections": "^2.0.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "hexo": "^7.1.1",
    "husky": "7",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.22.0",
    "react-router-dom": "^6.22.0",
    "sass": "^1.70.0",
    "sbg-utility": "^1.1.6",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "upath": "^2.0.1",
    "vite": "^5.0.12",
    "vite-plugin-markdown": "^2.2.0",
    "vitest": "^1.2.2"
  },
  "private": true,
  "packageManager": "yarn@4.1.1"
}
