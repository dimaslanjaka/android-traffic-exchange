{
  "type": "module",
  "name": "flowbite-react-admin-dashboard",
  "version": "1.0.0",
  "descrption": "flowbite-react admin dashboard",
  "homepage": "https://github.com/themesberg/flowbite-react-admin-dashboard",
  "bugs": "https://github.com/themesberg/flowbite-react-admin-dashboard/issues",
  "license": "UNLICENSED",
  "author": "Conner Davis <cdavis@scu.edu>",
  "repository": {
    "type": "git",
    "url": "https://github.com/themesberg/flowbite-react-admin-dashboard.git"
  },
  "scripts": {
    "build": "yarn typecheck && vite build",
    "dev": "cross-env-shell NODE_ENV=development vite --host",
    "start": "cross-env-shell NODE_ENV=development vite --host",
    "format": "prettier --check --ignore-path .gitignore --write .",
    "lint": "eslint --ignore-path .gitignore .",
    "preview": "vite preview --host",
    "typecheck": "tsc",
    "postinstall": "node -r dotenv/config --es-module-specifier-resolution=node --no-warnings _config.gen.cjs && husky install",
    "update": "yarn dlx npm-check-updates -u -x jsdom,chalk,hexo,deepmerge-ts,husky --enginesNode"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 8",
    "yarn": ">= 1"
  },
  "dependencies": {
    "@fancyapps/ui": "^5.0.35",
    "apexcharts": "^3.48.0",
    "axios": "^1.6.8",
    "axios-cache-interceptor": "^1.5.1",
    "classnames": "^2.5.1",
    "flowbite": "^2.3.0",
    "flowbite-react": "^0.8.0",
    "jquery-form-saver": "https://github.com/dimaslanjaka/jquery-form-saver#master",
    "react-apexcharts": "^1.4.1",
    "react-icons": "^5.0.1",
    "react-markdown": "^9.0.1",
    "react-simple-keyboard": "^3.7.107",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "simple-keyboard": "^3.7.73",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.24.1",
    "@types/google.accounts": "^0.0.14",
    "@types/node": "^20.12.4",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@types/sortablejs": "^1.15.8",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "binary-collections": "^2.0.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "flowbite-typography": "^1.0.3",
    "hexo": "^7.1.1",
    "husky": "7",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3",
    "sass": "^1.74.1",
    "sbg-utility": "^1.1.6",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4",
    "upath": "^2.0.1",
    "vite": "^5.0.12",
    "vite-plugin-markdown": "^2.2.0",
    "vitest": "^1.2.2"
  },
  "private": true,
  "packageManager": "yarn@4.1.1"
}
